<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ClearEditText">
	    <attr name="delete_icon" format="reference"/>
	    <attr name="character_upper" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="RelativeRadioGroup">
        <attr name="android:checkedButton" />
    </declare-styleable>

    <declare-styleable name="TitleView">
        <attr name="titleTextSize" format="dimension"/>
        <attr name="leftTextSize" format="dimension"/>
        <attr name="rightTextSize" format="dimension"/>
        <attr name="titleTextColor" format="color"/>
        <attr name="leftTextColor" format="color"/>
        <attr name="rightTextColor" format="color"/>
        <attr name="titleName" format="string"/>
        <attr name="leftEnable" format="boolean"/>
        <attr name="rightEnable" format="boolean"/>
        <attr name="leftClick" format="string"/>
        <attr name="rightClick" format="string"/>
        <attr name="leftElement" format="reference|string"/>
        <attr name="rightElement" format="reference|string"/>

    </declare-styleable>

</resources>
